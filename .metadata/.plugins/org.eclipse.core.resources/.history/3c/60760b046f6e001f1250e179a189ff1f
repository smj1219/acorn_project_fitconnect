package com.example.fitconnect.repository;

import java.util.List;

import com.fitconnect.trainer.dto.DietJournalDto;
import com.fitconnect.trainer.dto.ExerciseJournalDto;
import com.fitconnect.trainer.dto.MemberDto;
import com.fitconnect.trainer.dto.TrainerCalendarDto;

public interface TrainerCalendarDao {
	public List<TrainerCalendarDto> getCalenList(int trainer_num);
	public TrainerCalendarDto getCalender(int t_calendar_id);
	public void insert(TrainerCalendarDto dto);
	public void update(TrainerCalendarDto dto);
	public void delete(int t_calendar_id);
	
	public MemberDto getMember(MemberDto dto);
	public List<MemberDto> getMemberList(int trainer_num); 
	public List<ExerciseJournalDto> getExerJournalList(ExerciseJournalDto dto);
	public List<DietJournalDto> getDietJournalList(DietJournalDto dto);
	
	public void disconnect(int member_um);
	
}
